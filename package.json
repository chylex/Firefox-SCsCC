    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.14.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "web-ext": "^2.9.3",
    "webpack": "^3.12.0",
    "xregexp": "^4.2.0"
  }
}
